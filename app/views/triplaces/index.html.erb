<p id="notice"><%= notice %></p>

<h1>Triplaces</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @triplaces.each do |triplace| %>
      <tr>
        <td><%= link_to 'Show', triplace %></td>
        <td><%= link_to 'Edit', edit_triplace_path(triplace) %></td>
        <td><%= link_to 'Destroy', triplace, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Triplace', new_triplace_path %>
